{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "lsedu-plus/lesson-list",
	"version": "0.1.0",
	"title": "La Saphire Lesson List",
	"category": "la-saphire",
	"description": "Example static block scaffolded with Create Block tool.",
	"attributes": {
		"isSlider": {
			"type": "boolean",
			"default": false
		},
		"sliderScrollerColor": {
			"type": "object",
			"default": {
				"textColor": "#d3d3d3bf",
				"background": "#ffffff00"
			}
		},
		"columns": {
			"type": "number",
			"default": 3
		},
		"displayLayout": {
			"type": "string",
			"default": "list"
		}
	},
	"supports": {
		"align": [ "wide", "full" ],
		"color": {
			"text": false,
			"link": false,
			"background": true,
			"gradients": true
		},
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"usesContext": ["postType", "order", "orderBy", "inherit", "setTaxonomy"],
	"textdomain": "lsedu-plus",
	"editorScript": "file:./index.js",
	"style": "file:./index.css",
	"script": "file:./frontend.js"
}
