{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "lsedu-plus/lesson-query",
	"version": "0.1.0",
	"title": "La Saphire Lesson Query",
	"category": "la-saphire",
	"description": "Example static block scaffolded with Create Block tool.",
	"attributes": {
		"postType": {
			"type": "string",
			"default": "lesson"
		},
		"isSlider": {
			"type": "boolean",
			"default": false
		},
		"order": {
			"type": "string",
			"default": "asc"
		},
		"columns": {
			"type": "number",
			"default": 3
		},
		"orderBy": {
			"type": "string",
			"default": "title"
		},
		"listTitleType": {
			"type": "string",
			"default": "area"
		},
		"area": {
			"type": "string",
			"default": ""
		},
		"category": {
			"type": "string"
		},
		"intensity": {
			"type": "string"
		},
		"levels": {
			"type": "array",
			"items": {
				"type": "object"
			}
		},
		"durations": {
			"type": "array",
			"items": {
				"type": "object"
			}
		},
		"displayLayout": {
			"type": "string",
			"default": "list"
		}
	},
	"supports": {
		"align": [ "wide", "full" ],
		"color": {
			"text": false,
			"link": false,
			"background": true,
			"gradients": true
		},
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"providesContext": {
		"postType": "postType",
		"listTitleType": "listTitleType"
	},
	"textdomain": "lsedu-plus",
	"editorScript": "file:./index.js",
	"style": "file:./index.css"
}
